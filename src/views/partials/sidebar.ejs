<% if (user.usertype === 'Admin') { %>
    <script>
        $(document).ready(function () {
            // Make an AJAX GET request to fetch order counts
            $.ajax({
            type: 'GET',
            url: '/admin/orders/counts', // Replace with the actual route
            dataType: 'json',
            success: function (data) {
                // Update the HTML content with the received data
                $('#all').text(data.all);
                $('#pending').text(data.pending);
                $('#confirmed').text(data.confirmed);
                $('#processing').text(data.processing);
                $('#delivery').text(data.out);
                $('#delivered').text(data.delivered);
                $('#returned').text(data.returned);
                $('#failed').text(data.failed);
                $('#cancelled').text(data.cancelled);
                $('#scheduled').text(data.scheduled);
            },
            error: function (error) {
                console.error(error);
            },
            });
        });
    </script>
<% } else { %>
